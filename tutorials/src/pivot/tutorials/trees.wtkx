                                                <node text="Valentine's Day" icon="@page_white.png"/>
                                                <node text="Veteran's Day" icon="@page_white.png"/>
                                            </node>
                                            <node text="Anniversary" icon="@page_white.png"/>
                                            <node text="Birthday" icon="@page_white.png"/>
                                            <node text="Wedding" icon="@page_white.png"/>
                                        </node>
                                        <node text="Location" icon="@folder.png">
                                            <node text="Africa" icon="@folder.png"/>
                                            <node text="Antarctica" icon="@folder.png"/>
                                            <node text="Asia" icon="@folder.png"/>
                                            <node text="Australia" icon="@folder.png"/>
                                            <node text="Europe" icon="@folder.png"/>
                                            <node text="North America" icon="@folder.png"/>
                                            <node text="South America" icon="@folder.png">
                                                <node text="Peru" icon="@folder.png"/>
                                            </node>
                                        </node>
                                    </treeData>
                                    <nodeRenderer>
                                        <content:TreeViewNodeRenderer showIcon="true"/>
                                    </nodeRenderer>
                                </TreeView>
                            </view>
                        </ScrollPane>
                    </content>
                </Border>
            </FlowPane>

