    <FlowPane styles="{padding:4, spacing:10}">
        <FlowPane orientation="vertical">
            <Label text="Simple" styles="{font:'Verdana bold 11'}"/>
            <Border styles="{borderColor:'#999999', padding:0}">
                <ScrollPane preferredWidth="160" preferredHeight="240"
                    horizontalScrollBarPolicy="fillToCapacity">
                    <view>
                        <TreeView>
                            <treeData>
                                <node label="Numbers">
                                    <node label="0"/>
                                    <node label="1"/>
                                    <node label="2"/>
                                    <node label="3"/>
                                    <node label="4"/>
                                    <node label="5"/>
                                    <node label="6"/>
                                    <node label="7"/>
                                    <node label="8"/>
                                    <node label="9"/>
                                </node>
                                <node label="Letters">
                                    <node label="A"/>
                                    <node label="B"/>
                                    <node label="C"/>
                                    <node label="D"/>
                                    <node label="E"/>
                                    <node label="F"/>
                                    <node label="G"/>
                                    <node label="H"/>
                                    <node label="I"/>
                                    <node label="J"/>
                                    <node label="K"/>
                                    <node label="L"/>
                                    <node label="M"/>
                                    <node label="N"/>
                                    <node label="O"/>
                                    <node label="P"/>
                                    <node label="Q"/>
                                    <node label="R"/>
                                    <node label="S"/>
                                    <node label="T"/>
                                    <node label="U"/>
                                    <node label="V"/>
                                    <node label="W"/>
                                    <node label="X"/>
                                    <node label="Y"/>
                                    <node label="Z"/>
                                </node>
                            </treeData>
                            <nodeRenderer>
                                <content:TreeViewNodeRenderer showIcon="false"/>
                            </nodeRenderer>
                        </TreeView>
                    </view>
                </ScrollPane>
            </Border>
        </FlowPane>

        <FlowPane orientation="vertical">
            <Label text="Editable" styles="{font:'Verdana bold 11'}"/>
            <Border styles="{borderColor:'#999999', padding:0}">
                <ScrollPane wtkx:id="editableTreeViewScrollPane" preferredWidth="160" preferredHeight="240"
                    horizontalScrollBarPolicy="fillToCapacity">
                    <view>
                        <TreeView wtkx:id="editableTreeView">
                            <treeData>
                                <node label="Activity" icon="@folder.png">
                                    <node label="Games" icon="@folder.png">
                                        <node label="A very long game name that just serves to show how a wide tree node behaves" icon="@page_white.png"/>
                                        <node label="Air Hockey" icon="@page_white.png"/>
                                        <node label="Foosball" icon="@page_white.png"/>
                                        <node label="Ping Pong" icon="@page_white.png"/>
                                    </node>
                                    <node label="Sports" icon="@folder.png">
