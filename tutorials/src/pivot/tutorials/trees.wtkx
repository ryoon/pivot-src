                                        <node label="Basketball" icon="@page_white.png"/>
                                        <node label="Football" icon="@page_white.png"/>
                                        <node label="Ice Hockey" icon="@page_white.png"/>
                                        <node label="Soccer" icon="@page_white.png"/>
                                        <node label="Softball" icon="@page_white.png"/>
                                    </node>
                                    <node label="Camping" icon="@page_white.png"/>
