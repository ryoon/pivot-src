                                    <node label="Anniversary" icon="@page_white.png"/>
                                    <node label="Birthday" icon="@page_white.png"/>
                                    <node label="Wedding" icon="@page_white.png"/>
                                </node>
                                <node label="Location" icon="@folder.png">
                                    <node label="Africa" icon="@folder.png"/>
                                    <node label="Antarctica" icon="@folder.png"/>
                                    <node label="Asia" icon="@folder.png"/>
                                    <node label="Australia" icon="@folder.png"/>
                                    <node label="Europe" icon="@folder.png"/>
                                    <node label="North America" icon="@folder.png"/>
                                    <node label="South America" icon="@folder.png">
                                        <node label="Peru" icon="@folder.png"/>
                                    </node>
                                </node>
                            </treeData>
                            <nodeRenderer>
                                <content:TreeViewNodeRenderer showIcon="true"/>
                            </nodeRenderer>
                        </TreeView>
                    </view>
                </ScrollPane>
            </Border>
        </FlowPane>

