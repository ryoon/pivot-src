    xmlns:content="pivot.wtk.content" xmlns="pivot.wtk">
    <content>
        <FlowPane styles="{padding:4, spacing:10}">
            <FlowPane orientation="vertical">
                <Label text="Simple" styles="{fontBold:true}"/>
                <Border styles="{color:'#999999', padding:0}">
                    <content>
                        <ScrollPane preferredWidth="160" preferredHeight="240"
                            horizontalScrollBarPolicy="fillToCapacity">
                            <view>
                                <TreeView>
                                    <treeData>
                                        <node text="Numbers">
                                            <node text="0"/>
                                            <node text="1"/>
                                            <node text="2"/>
                                            <node text="3"/>
                                            <node text="4"/>
                                            <node text="5"/>
                                            <node text="6"/>
                                            <node text="7"/>
                                            <node text="8"/>
                                            <node text="9"/>
                                        </node>
                                        <node text="Letters">
                                            <node text="A"/>
                                            <node text="B"/>
                                            <node text="C"/>
                                            <node text="D"/>
                                            <node text="E"/>
                                            <node text="F"/>
                                            <node text="G"/>
                                            <node text="H"/>
                                            <node text="I"/>
                                            <node text="J"/>
                                            <node text="K"/>
                                            <node text="L"/>
                                            <node text="M"/>
                                            <node text="N"/>
                                            <node text="O"/>
                                            <node text="P"/>
                                            <node text="Q"/>
                                            <node text="R"/>
                                            <node text="S"/>
                                            <node text="T"/>
                                            <node text="U"/>
                                            <node text="V"/>
                                            <node text="W"/>
                                            <node text="X"/>
                                            <node text="Y"/>
                                            <node text="Z"/>
                                        </node>
                                    </treeData>
                                    <nodeRenderer>
                                        <content:TreeViewNodeRenderer showIcon="false"/>
                                    </nodeRenderer>
                                </TreeView>
                            </view>
                        </ScrollPane>
                    </content>
                </Border>
            </FlowPane>

            <FlowPane orientation="vertical">
                <Label text="Editable" styles="{fontBold:true}"/>
                <Border styles="{color:'#999999', padding:0}">
                    <content>
                        <ScrollPane wtkx:id="editableTreeViewScrollPane" preferredWidth="160" preferredHeight="240"
                            horizontalScrollBarPolicy="fillToCapacity">
                            <view>
                                <TreeView wtkx:id="editableTreeView">
                                    <treeData>
                                        <node text="Activity" icon="@folder.png">
                                            <node text="Games" icon="@folder.png">
                                                <node text="A very long game name that just serves to show how a wide tree node behaves" icon="@page_white.png"/>
                                                <node text="Air Hockey" icon="@page_white.png"/>
                                                <node text="Foosball" icon="@page_white.png"/>
                                                <node text="Ping Pong" icon="@page_white.png"/>
                                            </node>
                                            <node text="Sports" icon="@folder.png">
                                                <node text="Baseball" icon="@page_white.png"/>
                                                <node text="Basketball" icon="@page_white.png"/>
                                                <node text="Football" icon="@page_white.png"/>
                                                <node text="Ice Hockey" icon="@page_white.png"/>
                                                <node text="Soccer" icon="@page_white.png"/>
                                                <node text="Softball" icon="@page_white.png"/>
                                            </node>
                                            <node text="Camping" icon="@page_white.png"/>
